{
  "Name": "Java Runtime",
  "Author": "mail@mxritz.xyz",
  "UpdateUrl": null,
  "DonateUrl": "https://ko-fi.com/mxritzdev",
  "StartupCommand": "echo \"Starting\" && java -Xmx{{SERVER_MEMORY}}M -Dterminal.jline=false -Dterminal.ansi=true -jar {{JARFILE}}",
  "OnlineDetection": "Starting",
  "StopCommand": "^C",
  "InstallShell": "/bin/bash",
  "InstallDockerImage": "debian:latest",
  "InstallScript": "#! /bin/bash\necho Done",
  "ParseConfiguration": "[{\"Type\":\"file\",\"File\":\"config.yml\",\"Configuration\":{\"listener\":\" listeners[0].host\\\": \\\"0.0.0.0:{{server.build.default.port}}\"}}]",
  "AllocationsNeeded": 1,
  "Variables": [
    {
      "Key": "JARFILE",
      "DefaultValue": "app.jar",
      "DisplayName": "Jarfile",
      "Description": "Specifies what .jar to run on startup",
      "AllowView": true,
      "AllowEdit": true,
      "Filter": "^[^\\s]+\\.jar$"
    }
  ],
  "DefaultDockerImage": 6,
  "AllowDockerImageChange": true,
  "DockerImages": [
    {
      "DisplayName": "Java 8",
      "Name": "ghcr.io/mxritzdev/java:8",
      "AutoPull": true
    },
    {
      "DisplayName": "Java 11",
      "Name": "ghcr.io/mxritzdev/java:11",
      "AutoPull": true
    },
    {
      "DisplayName": "Java 16",
      "Name": "ghcr.io/mxritzdev/java:16",
      "AutoPull": true
    },
    {
      "DisplayName": "Java 17",
      "Name": "ghcr.io/mxritzdev/java:17",
      "AutoPull": true
    },
    {
      "DisplayName": "Java 18",
      "Name": "ghcr.io/mxritzdev/java:18",
      "AutoPull": true
    },
    {
      "DisplayName": "Java 19",
      "Name": "ghcr.io/mxritzdev/java:19",
      "AutoPull": true
    },
    {
      "DisplayName": "Java 20",
      "Name": "ghcr.io/mxritzdev/java:20",
      "AutoPull": true
    },
    {
      "DisplayName": "Java 21",
      "Name": "ghcr.io/mxritzdev/java:21",
      "AutoPull": true
    },
    {
      "DisplayName": "Java 22",
      "Name": "ghcr.io/mxritzdev/java:22",
      "AutoPull": true
    }
  ]
}
